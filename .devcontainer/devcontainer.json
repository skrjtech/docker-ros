
{
	"name": "khi robot",
	"dockerComposeFile": "../.docker/docker-compose-noetic.yml",
	"service": "khi_robot",
	"workspaceFolder": "/catkin_ws",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-python.vscode-pylance",
				"ms-vscode.cmake-tools"
			]
		},
		"settings": {
			"python.pythonPath": "/usr/local/bin/python"	
		}
	}
}

